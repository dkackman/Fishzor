{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Blazor App",
            "type": "blazorwasm",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Fishzor/Fishzor/bin/Debug/net8.0/Fishzor.dll",
            "cwd": "${workspaceFolder}/Fishzor/Fishzor",
            "browser": "edge"
        },
        {
            "name": ".NET Core Launch",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Fishzor/Fishzor/bin/Debug/net8.0/Fishzor.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Fishzor/Fishzor",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        }
    ]
}